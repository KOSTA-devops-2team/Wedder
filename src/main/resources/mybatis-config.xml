<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration
        PUBLIC "-//mybatis.org/DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd" >

<configuration>
    <settings>
        <!-- snake to camel -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <!-- 쿼리에 보내는 파라미터가 null인 경우 에러 발생 방지 -->
        <setting name="jdbcTypeForNull" value="NULL"/>
    </settings>
    <typeAliases>
        <typeAlias  alias="CustomerDTO" type="kr.co.wedder.customer.domain.CustomerDTO" />
        <typeAlias  alias="UserDto" type="kr.co.wedder.user.domain.UserDto" />
        <typeAlias alias="CompanyDto" type="kr.co.wedder.company.domain.CompanyDto" />
        <typeAlias  alias="PackageDto" type="kr.co.wedder.sdmpackage.domain.PackageDto" />
        <typeAlias  alias="PackageDetailDto" type="kr.co.wedder.sdmpackage.domain.PackageDetailDto" />
        <!--calendar alias  -->
        <typeAlias alias="CompanyScheduleDto" type="kr.co.wedder.calendar.domain.CompanyScheduleDto"/>
        <typeAlias alias="SearchItem" type="kr.co.wedder.calendar.domain.SearchItem"/>
        <!--mypage alias  -->
        <typeAlias alias="MyPageDTO" type="kr.co.wedder.mypage.domain.MyPageDTO"/>
        <typeAlias alias="CompanyDto" type="kr.co.wedder.mypage.domain.CompanyDto"/>
        <typeAlias alias="ReservationDto" type="kr.co.wedder.mypage.domain.ReservationDto"/>
        <typeAlias alias="HistoryDto" type="kr.co.wedder.mypage.domain.HistoryDto"/>
        <typeAlias alias="VisitCriteria" type="kr.co.wedder.mypage.domain.VisitCriteria"/>
    </typeAliases>
</configuration>